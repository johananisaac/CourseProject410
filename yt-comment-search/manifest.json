{
  "name": "YouTube Comment Search Engine",
  "description": "A tool for searching comments in YouTube",
  "version": "1.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon":{
      "16": "images/youtubeicon.png"
    } 
  },
  "icons": {
    "16": "images/youtubeicon.png",
    "32": "images/youtubeicon.png",
    "48": "images/youtubeicon.png",
    "128": "images/youtubeicon.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}